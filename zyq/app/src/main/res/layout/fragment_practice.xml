<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context="com.hcyclone.zyq.PracticeFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/fontFamilyMedium"
        android:padding="16dp"
        android:text="level 1"
        android:textSize="16sp"/>
    <Button
        android:id="@+id/warmup_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:text="warmup"/>
    <Button
        android:id="@+id/practice_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:text="@string/fragment_practice_title"/>

</LinearLayout>
